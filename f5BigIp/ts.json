{
    "class": "Telemetry",
    "My_System": {
        "class": "Telemetry_System",
        "systemPoller": {
          "interval": 60,
          "actions": [
            {
              "setTag": {
                "facility": "ECK-Azure-AKS"
              },
              "locations": {
                "system": true
              }
            }
          ]
        }
    },
    "controls": {
        "class": "Controls",
        "logLevel": "info"
    },
    "My_Poller": {
    	"class": "Telemetry_System_Poller",
    	"interval": 0
    },
    "My_Listener": {
        "class": "Telemetry_Listener",
        "port": 6514
    },
    "metrics": {
    	"class": "Telemetry_Pull_Consumer",
    	"type": "Prometheus",
        "systemPoller": ["My_Poller"]
    },
    "My_Consumer": {
        "class": "Telemetry_Consumer",
        "type": "Generic_HTTP",
        "host": "20.220.251.246",
        "protocol": "http",
        "port": 7080,
        "path": "/",
        "method": "POST",
        "headers": [
            {
                "name": "content-type",
                "value": "application/json"
            }
        ],
        "outputMode": "processed"
    }
}
